@inproceedings{Dasgupta2008,
address = {New York, New York, USA},
annote = {This paper $\backslash$cite\{Dasgupta2008\} examines the communications patterns of millions of mobile phone users, allowing us to study the underlying social network in a large-scale communication network. Traditional solutions have used data mining techniques that create a customer profile from her calling pattern, and then predict the probability of churn based on certain attributes of a subscriber. In this case social connections of users are analysed. Using only calls data from CDR-s, social network graph is made and based on intensity of connetions to past churners future churners are predicted. The method used is called Spreading Activation technique (SPA). Results (lift curve) show that SPA is generally successful in making correct predictions about 50-60\% of future churners, by contacting a relatively small fraction (10-20\%) of subscribers. In future they plan to extract additional CDR information (e.g. inter-region calls, SMS records, etc.). They also compare this method to J48 Weka classifier (decision trees). A traditional (label) attributes-based classifier loses out to SPA because it fails to adequately learn all the ties in the network neighbourhood. Further gains in the prediction accuracy can be potentially achieved by applying link mining techniques, based on collective classification.},
author = {Dasgupta, Koustuv and Singh, Rahul and Viswanathan, Balaji and Chakraborty, Dipanjan and Mukherjea, Sougata and Nanavati, A.A. and Joshi, Anupam},
booktitle = {Proceedings of the 11th international conference on Extending database technology: Advances in database technology},
doi = {10.1145/1353343.1353424},
file = {::},
isbn = {9781595939265},
pages = {668--677},
publisher = {ACM},
title = {{Social ties and their relevance to churn in mobile telecom networks}},
url = {http://portal.acm.org/citation.cfm?id=1353424},
year = {2008}
}
